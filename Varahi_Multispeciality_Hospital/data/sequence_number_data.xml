        <field name="code">hospital.employee.sequence</field>
        <field name="prefix">VMSH-Emp</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="hospital_doctor_sequence" model="ir.sequence">
        <field name="name">Hospital Doctor Sequence</field>
        <field name="code">hospital.doctor.sequence</field>
        <field name="prefix">VMSH-Doc</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>